<% objects.each do |object| %>
  <div class="card">
    <div class="card-body">
      <h5 class="card-title"><%= link_to object.teacher.name, object.teacher, data: { turbo_frame: '_top' } %></h5>
      <h6 class="card-subtitle mb-2 text-muted">
        <%= object.course_title %>
      </h6>
      Classroom: <%= link_to object.classroom.full_name, object.classroom, data: { turbo_frame: '_top' } %>
      <br>
      Block: <%= link_to object.block.name, object.block, data: { turbo_frame: '_top' } %>
    </div>
  </div>
<% end %>
