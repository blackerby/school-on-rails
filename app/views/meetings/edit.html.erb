<h1>Edit <%= @teacher.name%>'s <%= @meeting.block.name %> Block Class</h1>
<%= form_with(model: [@teacher, @meeting]) do |f| %>
  <%= render 'shared/errors', object: @meeting %>
  <%= f.label :course_title, class: 'form-label' %>
  <%= f.text_field :course_title, class: 'form-control me-2' %>
  <%= f.label :classroom, class: 'form-label' %>
  <%= f.collection_select :classroom_id, Classroom.all, :id, :name, {}, { class: 'form-select' } %>
  <%= f.submit class: 'mt-3 btn btn-primary'%>
<% end %>
